
page{
	background-color: #f3f3f3;
}
uni-view{
		font-size: 30rpx;
}
#tab-bar {
		border-bottom: none;
		background: #FFFFFF;
}
.uni-scroll-view {
		overflow: Scroll;
		overflow-x: hidden
}
#tab-bar .active{
		color: #dd4036 !important;
		border-bottom: 4rpx solid #dd4036!important;
}
.swiper-tab-list:first-child {
		margin-left: 59rpx;
}
.swiper-tab-list {
		width: 120rpx;
		text-align: center;
		margin: 0 30rpx;
		/* padding: 18upx 0; */
		font-size: 30rpx;
		color: #333333;
}
    /* 轮播图 */
.part2{  
    width: 100%;  
    height: 300rpx;   
    overflow: hidden;  
    position: relative;
}
.part2:after{  
    content: " ";  
    height: 40rpx;  
    border-radius: 50%;  
    background: #f9f9f9;  
    position: absolute;  
    bottom: -20rpx;  
    left: -40rpx;  
    right: -40rpx;
}
.banner-box{  
    width: 100%;  
    height: 100%;
}
.banner-image{  
    width: 100%;  
    height: 100%;
}
.serve{
	/* margin: 0 30upx; */
	/* margin-bottom: 120upx; */
}
.serve-list .serve-tittle{
	font-size: 38rpx;
	color: #dd3e34;
	padding: 20rpx 0;
}
.serve-content{
	background-color: #fff;
	border: solid 1px #f3f3f3;
}
.serve-img{
	width: 100%;
}
.serve-text{
	margin: 30rpx;
}
.serve-text .serve-txt{
	color: #333333;
	font-size: 32rpx;
	line-height: 48rpx;
}
.member{
	display: inline-block;
	background-color: #fff5e4;
	border-radius: 1px;
	color: #b37d1d;
	font-size: 20rpx;
	padding:0 10rpx;
	margin-right: 20rpx;
}
.member ._img{
	width: 22rpx;
	height: 22rpx;
	margin-right: 10rpx;
	position: relative;
	top:5rpx;
}
.num{
	font-weight: bold;
}
.servec-bottom{
	position: relative;
}
.experiential .experiential-tit{
	background-color: #dd3e34;
	border-radius: 6rpx;
	font-size: 18rpx;
	color: #fff;
	padding: 2rpx 6rpx;
	margin-right: 10rpx;
}
.experiential-price{
	color: #dd3e34;
	font-size: 24rpx;
}
.experiential-price .num{
	font-size: 40rpx;
}
.share{
	position: absolute;
	top:10rpx;
	right: 0;
	font-size: 32rpx;
	color: #dd3e34;
}
.share ._img{
	width: 44rpx;
	height: 44rpx;
	margin-right: 10rpx;
	vertical-align: middle;
	margin-top: -4rpx;
}
.banner ._img{
	width: 100%;
	margin: 20rpx 0 10rpx 0;
}
.serve-bottom{
	background-color: #fff;
	/* position: relative; */
}
.group{
	padding: 20rpx 30rpx;
	position: relative;
}
.group-img{
	width: 204rpx;
	height: 204rpx;
}
.group-img ._img{
	width: 100%;
	height: 100%;
}
.groupRight{
	position: absolute;
	width: 440rpx;
	left: 270rpx;
	top:16rpx;
	border-bottom: 1px solid #f3f3f3;
	padding-bottom: 10rpx;
}
.group-title{
	font-size: 28rpx;
	line-height: 40rpx;
	letter-spacing: 1px;
	color: #333333;
}
.group-cont{
	font-size: 22rpx;
	color: #999999;
}
.spell{
	color: #b37d1d;
	font-size: 20rpx!important;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.spell ._span{
	margin-top: 10rpx;
}
.group-bottom{
	margin-top: 10rpx;
}
.go-group{
	background-color: #dd3e34;
	border-radius: 10rpx;
	color: #fff;
	padding: 2rpx 10rpx;
	font-size: 30rpx;
}

