
page{
	background-color: #f3f3f3;
}
.with{
	background-color: #fff;
	margin:20rpx 30rpx;
	height: 440rpx;
	padding: 50rpx 60rpx;
}
.tit{
	font-size: 26rpx;
	color: #333333;
}
.money {
	border-bottom: 1px solid #d4d4d4;
	padding: 20rpx 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.money ._span{
	font-size: 60rpx;
	color: #333333;
	font-weight: bold;
}
.money input{
	font-size: 60rpx;
	font-weight: bold;
	height: 100rpx;
	margin-left: 20rpx;
}
.tite{
	color: #999999;
	font-size: 26rpx;
	padding: 20rpx 0;
}
.tite ._span{
	color: #5f729a;
}
.btn{
	text-align: center;
	margin-top: 40rpx;
}
.btn .invite{
	width: 60%;
	border-radius: 30rpx;
	margin-left: 18%;
}
.content view{
	padding: 20rpx 40rpx;
	font-size: 30rpx!important;
	color: #333;
}

