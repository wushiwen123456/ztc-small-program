.serlist{
	  display: -webkit-box;
	  display: -webkit-flex;
	  display: flex;
	  margin: 10rpx 30rpx;
	  -webkit-box-pack: justify;
	  -webkit-justify-content: space-between;
	          justify-content: space-between;
	  -webkit-box-align: center;
	  -webkit-align-items: center;
	          align-items: center;
	  margin-top: 1rpx;
	  background-color: #fff;
	  border-radius: 10rpx;
}
.serlist .img{
	  margin-left: 30rpx;
	  margin-right: 20rpx;
	  width: 48rpx;
	  height: 48rpx;
	  vertical-align: middle;
}
