
page{
	 background-color: #f3f3f3;
}
.cold{
}
.cold .tite{
	  padding: 20rpx 30rpx;
	  background-color: #fff;
	  font-size: 32rpx;
}
.serlist{
	  margin-top: 1rpx;
	   background-color: #fff;
}
.serlist .listv{
	  display: -webkit-box;
	  display: -webkit-flex;
	  display: flex;
}
.serlist .listv .img{
	  margin-top: 22rpx;
	  margin-left: 30rpx;
	  margin-right: 20rpx;
	  width: 48rpx;
	  height: 48rpx;
	  vertical-align: middle;
}
.serlist .input{
	  border-bottom: 1rpx solid #d4d4d4;
	  padding: 20rpx 0;
	  width: 100%;
}
.tag{
	  margin-left: 98rpx;
}
.tag ._span{
	  border-radius: 30rpx;
	  border: solid 1rpx #999999;
	  font-size: 24rpx;
	  color: #999999;
	  padding: 2rpx 16rpx;
	  margin: 30rpx 0;
	  display: inline-block;
	  margin-right: 20rpx;
}
.technicians{
	  margin-bottom: 20rpx;
	  margin-top: 1px;
}
.technicians .tech{
	  padding: 20rpx 30rpx;
	  background-color: #fff;
	  position: relative;
}
.technicians .tech .right{
	  right: 30rpx;
	  top:20rpx;
}
.cont{
	  margin-top: 2rpx;
	  background-color: #fff;
	  display: -webkit-box;
	  display: -webkit-flex;
	  display: flex;
	  padding: 20rpx 30rpx;
}
.cont .img{
	  width: 130rpx;
	  height: 130rpx;
	  background-color: #f3f3f3;
	  border: solid 1rpx #d4d4d4;
	  border-radius: 50%;
}
.cont .img ._img{
	  border-radius: 50%;
}
.cont .img ._span{
	  display: inline-block;
	  background-color: #f3f3f3;
	  border: solid 1rpx #d4d4d4;
	  border-radius: 50%;
}
.cont .txt{
	  margin-left: 20rpx;
}
.cont .txt .tit .name{
	  font-size: 36rpx;
}
.cont .txt .tit .tip{
	  background-color: #fff5e4;
	  border-radius: 1rpx;
	  font-size: 20rpx;
	  letter-spacing: 1rpx;
	  color: #b37d1d;
	  padding: 2rpx 10rpx;
	  margin-left: 20rpx;
}
.cont .txt .text{
	  font-size: 22rpx;
	  color: #666666;
}
.times{
	  position: relative;
	  background-color: #fff;
	  margin-top: 20rpx;
	  padding: 20rpx 30rpx;
	  margin-bottom: 170rpx;
}
.times .right{
	  right: 30rpx;
	  top:20rpx;
}
.times .right image{
	  width: 34rpx;
	  height: 34rpx;
}
.titel{
  	  padding: 20rpx 0;
  	  background-color: #fff;
  	  margin-top: 2rpx;
}
.titel .top{
  	  font-size: 28rpx;
  	  color: #333333;
}
.titel .bottom{
  	  font-size: 28rpx;
  	  color: #999999;
}
.titel .gray{
  	font-size: 22rpx;
  	margin-left: 30rpx;
  	letter-spacing: 0;
}

