
page{
		background-color: #f3f3f3;
}
.reser{
    	margin-top: 1rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		padding: 20rpx 30rpx;
		background-color: #fff;
}
.reser .img{
		width: 178rpx;
	    height: 178rpx;
		margin-right: 20rpx;
}
.reser .cont{
	    width: 400rpx;
}
.reser .cont .tit{
		font-size: 30rpx;
		color: #333333;
}
.reser .cont .txt{
		font-size: 24rpx;
		color: #999999;
}
.reser .cont .txt ._img{
		width: 22rpx;
	    height: 22rpx;
		margin-right: 10rpx;
		vertical-align: middle;
}
.tite{
		  padding: 20rpx 30rpx;
		  background-color: #fff;
		  margin-top: 20rpx;
		  margin-bottom: 2rpx;
		  font-size: 32rpx;
}
.serlist{
		  margin-top: 1rpx;
		   background-color: #fff;
}
.serlist .listv{
		  display: -webkit-box;
		  display: -webkit-flex;
		  display: flex;
}
.serlist .listv .img{
		  margin-top: 22rpx;
		  margin-left: 30rpx;
		  margin-right: 20rpx;
		  width: 48rpx;
		  height: 48rpx;
		  vertical-align: middle;
}
.serlist .input{
		  border-bottom: 1rpx solid #f3f3f3;
		  padding: 20rpx 0;
		  width: 100%;
		  font-size: 28rpx;
}
.serlist .input ._span{
		font-size: 30rpx;
}
.address{
		width: 70%;
		display: inline-block;
		font-size: 24rpx !important;
		vertical-align: middle;
}
.doors{
		background-color: #fff;
		position: relative;
		padding: 20rpx 30rpx;
}
.doors .right{
		right: 30rpx;
		top:20rpx;
}
.technicians{
		  margin-top: 20rpx;
		  margin-bottom: 220rpx;
}
.technicians .tech{
		  padding: 20rpx 30rpx;
		  background-color: #fff;
		  position: relative;
}
.technicians .tech .right{
		  right: 30rpx;
		  top:20rpx;
}
.technicians .cont{
		  margin-top: 2rpx;
		  background-color: #fff;
		  display: -webkit-box;
		  display: -webkit-flex;
		  display: flex;
		  padding: 20rpx 30rpx;
}
.technicians .cont .img{
		  width: 130rpx;
		  height: 130rpx;
		  background-color: #f3f3f3;
		  border: solid 1rpx #d4d4d4;
		  border-radius: 50%;
}
.technicians .cont .txt{
		  margin-left: 20rpx;
}
.technicians .cont .txt .tit .name{
		  font-size: 36rpx;
}
.technicians .cont .txt .tit .tip{
		  background-color: #fff5e4;
		  border-radius: 1rpx;
		  font-size: 20rpx;
		  letter-spacing: 1rpx;
		  color: #b37d1d;
		  padding: 2rpx 10rpx;
		  margin-left: 20rpx;
}
.technicians .cont .txt .text{
		  font-size: 22rpx;
		  color: #666666;
}

